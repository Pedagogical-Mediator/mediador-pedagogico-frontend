\n              }\n            );\n          } else {\n            this.$http.post(\"turmas\", this.turma).then(\n              () => {\n               // window.location.href = \"/?idAlert=turmaSuccess\";\n                this.$router.push({\n                name: \"listagemTurma\",\n                query: { idAlert: \"cadastrarSuccess\" }\n              }); \n              },\n              () => {\n               // window.location.href = \"/?idAlert=turmaError\";\n                this.$router.push({\n                name: \"listagemTurma\",\n                query: { idAlert: \"cadastrarSuccess\" }\n              }); \n              }\n            );\n          }\n        }\n      });\n    }\n  },\n  data: () => ({\n    turma: new Turma(),\n    turmaEditar: null\n  })\n};\n</script>\n<style scoped>\na {\n  text-decoration: none;\n  color: white;\n}\n</style>"]}]}