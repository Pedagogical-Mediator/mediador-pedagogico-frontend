he image file as a data URL.\n      reader.readAsBinaryString(f);\n    },\n\n    submit() {\n      this.$validator.validateAll().then(valid => {\n        if (!valid) {\n          alert(\"Preencha os campos corretamente!\");\n        } else {\n          //renderizando imagem\n          this.gremio.imagem = document.getElementById(\"base64\").src.substr(22);\n\n          this.$http.put(\"gremio\", this.gremio).then(\n            () => {\n              window.location.href = \"/?idAlert=gremioSuccess\";\n\n              /* this.$router.push({\n                name: \"home\",\n                query: { idAlert: \"success\" }\n              }); */\n            },\n            err => {\n              console.error(err);\n              window.location.href = \"/?idAlert=gremioError\";\n              /*this.$router.push({ name: \"home\", query: { idAlert: \"gremioError\" } })*/\n            }\n          );\n        }\n      });\n    }\n  },\n  data: () => ({\n    cadastrar: false,\n    gremio: new Gremio(),\n    imagemUploadUrl: require(\"@/assets/upload-icon.png\"),\n    customToolbar: [\n      [\"bold\", \"italic\", \"underline\"],\n      [{ list: \"ordered\" }, { list: \"bullet\" }],\n      [{ align: \"\" }, { align: \"justify\" }, { align: \"right\" }]\n    ]\n  })\n};\n</script>\n<style scoped>\na {\n  text-decoration: none;\n  color: white;\n}\n</style>"],"sourceRoot":"src/gremio/components/box"}]}