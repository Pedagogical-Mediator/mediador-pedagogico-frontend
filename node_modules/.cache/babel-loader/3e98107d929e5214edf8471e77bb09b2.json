\n      <v-layout row>\n        <v-flex md8>\n          <v-layout d-flex mt-4>\n            <v-flex md10 class=\"white lighten-2\">\n              <v-form class=\"ml-4\">\n                <v-flex md12 mt-3>\n                  <h1 class=\"input\">link do Gremio</h1>\n                  <v-text-field\n                    name=\"linkDoGremio\"\n                    v-validate=\"'required'\"\n                    :error-messages=\"errors.collect('linkDoGremio')\"\n                    data-vv-name=\"linkDoGremio\"\n                    background-color=\"white\"\n                    v-model=\"gremio.linkDoGremio\"\n                    required\n                    lazy-validation\n                  ></v-text-field>\n                </v-flex>\n                <v-flex md12 mt-3>\n                  <h1 class=\"input\">Descrição</h1>\n\n                  <vue-editor v-model=\"gremio.descricao\" :editor-toolbar=\"customToolbar\"></vue-editor>\n\n                  <v-layout row wrap justify-end>\n                    <router-link :to=\"{path:'/'}\">\n                      <v-btn dark>Cancelar</v-btn>\n                    </router-link>\n\n                    <v-btn dark @click=\"submit()\">Publicar</v-btn>\n                  </v-layout>\n                </v-flex>\n              </v-form>\n            </v-flex>\n          </v-layout>\n        </v-flex>\n        <v-layout column align-center justify-center class=\"grey lighten-1\" md4>\n          <v-flex md2>\n            <img :height=\"'120px'\" id=\"base64\" :src=\"imagemUploadUrl\" />\n          </v-flex>\n\n          <v-btn color=\"black\" dark>\n            <v-flex md1>\n              <input\n                class=\"md1\"\n                style=\"opacity:0; position:absolute; float:left;\"\n                @change=\"onFileChange1\"\n                :type=\"'file'\"\n              />\n            </v-flex>Inserir Imagem\n          </v-btn>\n        </v-layout>\n      </v-layout>\n    </v-container>\n  </div>\n</template>\n<script